{
	"name": "kha",
	"displayName": "Kha",
	"description": "Kha support",
	"version": "25.3.0",
	"publisher": "kodetech",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"Kha",
		"Haxe"
	],
	"activationEvents": [
		"workspaceContains:khafile.js",
		"onCommand:kha.init",
		"onCommand:kha.compile",
		"onCommand:kha.findKha",
		"onCommand:kha.findKhaElectron"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "kha.init",
				"title": "Init Project",
				"category": "Kha"
			},
			{
				"command": "kha.findKha",
				"title": "Locate Kha folder",
				"category": "Kha"
			},
			{
				"command": "kha.findKhaElectron",
				"title": "Locate the downloaded electron executable",
				"category": "Kha"
			},
			{
				"command": "kha.updateKha",
				"title": "Update extension-internal Kha",
				"category": "Kha"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Kha",
			"properties": {
				"kha.khaPath": {
					"type": "string",
					"default": "",
					"description": "Points to a Kha directory."
				},
				"kha.ffmpeg": {
					"type": "string",
					"default": "",
					"description": "Path to ffmpeg for extended audio and video format support."
				},
				"kha.buildDir": {
					"type": "string",
					"default": "build",
					"description": "Relative path to the Kha build dir for the project."
				},
				"kha.defaultTarget": {
					"type": "string",
					"description": "Default completion and build target.",
					"default": "HTML5 (Electron)",
					"enum": [
						"HTML5 (Electron)",
						"HTML5 (Web)",
						"Krom",
						"Kinc",
						"Android (Java)",
						"Flash",
						"HTML5-Worker",
						"Java",
						"Node.js",
						"Unity",
						"WPF"
					]
				}
			}
		},
		"taskDefinitions": [
			{
				"type": "Kha",
				"required": [
					"target"
				],
				"properties": {
					"target": {
						"type": "string",
						"description": "The target to build for."
					}
				}
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Kode/vscode-kha"
	},
	"extensionDependencies": [
		"nadako.vshaxe"
	],
	"runtimeDependencies": [
